import java.io.PrintWriter;
import java.net.Socket;


public class OutputWriter implements Runnable {
	
	Socket s;
	PrintWriter p;
	
	public OutputWriter(Socket s){
		this.s = s;
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		p = new PrintWriter(s.getOutputStream());
		
		while(true){
			
		}
	}

}
