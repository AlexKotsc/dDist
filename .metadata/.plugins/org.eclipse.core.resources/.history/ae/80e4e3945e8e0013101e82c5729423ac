package ex9;

import java.net.ServerSocket;

public class ListenRunnable implements Runnable {
	
	int port;
	ServerSocket server;
	
	public ListenRunnable(int port){
		this.port = port;
	}

	@Override
	public void run() {
		server = new ServerSocket(port);
		while(true){
			Socket incoming = server.accept();
			
		}
		
	}

}
